<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <Version>2.0.1-beta-017</Version>
    <Authors>zhenlei520</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>zhenlei520</Company>
    <Description>dotnet core mysql帮助类库</Description>
    <RepositoryUrl>https://github.com/zhenlei520/System.Extension.Core</RepositoryUrl>
    <PackageProjectUrl>https://www.nuget.org/packages/EInfrastructure.Core.MySql/</PackageProjectUrl>
    <Copyright>Copyright ©zhenlei520 2020</Copyright>
    <PackageIconUrl>https://secure.gravatar.com/avatar/932ca0f8eaf4df1aaf221f9a1a897569?s=512</PackageIconUrl>
    <Title>mysql基础类库，实现基类增删改查方法</Title>
    <PackageTags>EInfrastructure EInfrastructure.Mysql</PackageTags>
    <NeutralLanguage>zh-CN</NeutralLanguage>

  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\EInfrastructure.Core.MySql.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\EInfrastructure.Core.MySql.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.1.0"/>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="2.1.0"/>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Infrastructure\src\EInfrastructure.Core\EInfrastructure.Core.csproj"/>
  </ItemGroup>

</Project>
