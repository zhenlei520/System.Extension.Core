<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <Version>2.0.1-beta-021</Version>
    <Authors>zhenlei520</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>zhenlei520</Company>
    <Description>dotnet core 基础类库</Description>
    <RepositoryUrl>https://github.com/zhenlei520/System.Extension.Core</RepositoryUrl>
    <PackageProjectUrl>https://www.nuget.org/packages/EInfrastructure.Core.Validation.AspNetCore/</PackageProjectUrl>
    <Copyright>Copyright ©zhenlei520 2020</Copyright>
    <Title>参数校验基础类库</Title>
    <PackageTags>EInfrastructure.Core EInfrastructure.Core.Validation EInfrastructure ECommon</PackageTags>
    <NeutralLanguage>zh-CN</NeutralLanguage>
    <PackageIcon>packageIcon.png</PackageIcon>
    <PackageIconUrl />

  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\EInfrastructure.Core.Validation.AspNetCore.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\EInfrastructure.Core.Validation.AspNetCore.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Web\AspNetCore\src\EInfrastructure.Core.AspNetCore\EInfrastructure.Core.AspNetCore.csproj" />
    <ProjectReference Include="..\EInfrastructure.Core.Validation\EInfrastructure.Core.Validation.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\..\packageIcon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
