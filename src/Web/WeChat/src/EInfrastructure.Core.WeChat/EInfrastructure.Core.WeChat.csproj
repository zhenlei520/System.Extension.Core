<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
   <Version>2.0.1-beta-018</Version>
<Authors>zhenlei520</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>zhenlei520</Company>
    <Description>dotnet core 微信帮助类库</Description>
    <RepositoryUrl>https://github.com/zhenlei520/System.Extension.Core</RepositoryUrl>
    <PackageProjectUrl>https://www.nuget.org/packages/EInfrastructure.Core.WebChat/</PackageProjectUrl>
    <Copyright>Copyright ©zhenlei520 2020</Copyright>
    <Title>微信公众号帮助类库</Title>
    <PackageIconUrl>https://secure.gravatar.com/avatar/932ca0f8eaf4df1aaf221f9a1a897569?s=512</PackageIconUrl>
    <PackageTags>wechat</PackageTags>
    <NeutralLanguage>zh-CN</NeutralLanguage>

  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\EInfrastructure.Core.WebChat.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\EInfrastructure.Core.WebChat.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Infrastructure\src\EInfrastructure.Core\EInfrastructure.Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="RestSharp" Version="106.3.1" />
  </ItemGroup>
</Project>
